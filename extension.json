{
  "identifier": "teriyaki.LaravelArtisan",
  "name": "Laravel Artisan",
  "organization": "Teriyaki",
  "description": "Run artisan commands from the command palette",
  "repository": "https://github.com/TeriyakiBomb/nova-artisan",
  "bugs": "https://github.com/TeriyakiBomb/nova-artisan/issues",
  "funding": "https://ko-fi.com/ashteriyaki",
  "version": "1.3",
  "categories": ["commands", "tasks"],
  "license": "MIT",

  "main": "main.js",

  "activationEvents": ["onLanguage:php"],

  "entitlements": {
    "process": true,
    "filesystem": "readwrite"
  },

  "commands": {
    "extensions": [
      {
        "title": "Cache config",
        "command": "laravel-artisan.configCache"
      },
      {
        "title": "Clear config cache",
        "command": "laravel-artisan.configClear"
      },
      {
        "title": "Clear cache",
        "command": "laravel-artisan.cacheClear"
      },
      {
        "title": "Forget cache",
        "command": "laravel-artisan.cacheForget"
      },
      {
        "title": "Forget cache",
        "command": "laravel-artisan.cacheForget"
      },
      { "separator": true },
      {
        "title": "Seed database",
        "command": "laravel-artisan.dbSeed"
      },
      {
        "title": "Migrate",
        "command": "laravel-artisan.migrate"
      },
      {
        "title": "Migrate - fresh",
        "command": "laravel-artisan.migrateFresh"
      },
      {
        "title": "Migrate - refresh",
        "command": "laravel-artisan.migrateRefresh"
      },
      {
        "title": "Migrate - reset",
        "command": "laravel-artisan.migrateReset"
      },
      {
        "title": "Migrate - rollback",
        "command": "laravel-artisan.migrateRollback"
      },
      {
        "title": "Wipe - database",
        "command": "laravel-artisan.dbWipe"
      },
      { "separator": true },
      {
        "title": "Prune models",
        "command": "laravel-artisan.modelPrune"
      },
      { "separator": true },
      {
        "title": "Create notifications migration",
        "command": "laravel-artisan.notificationsTable"
      },
      {
        "title": "Create cast",
        "command": "laravel-artisan.makeCast"
      },
      {
        "title": "Create channel",
        "command": "laravel-artisan.makeChannel"
      },
      {
        "title": "Create command",
        "command": "laravel-artisan.makeCommand"
      },
      {
        "title": "Create component",
        "command": "laravel-artisan.makeComponent"
      },
      {
        "title": "Create controller",
        "command": "laravel-artisan.makeController"
      },
      {
        "title": "Create event",
        "command": "laravel-artisan.makeEvent"
      },
      {
        "title": "Create exception",
        "command": "laravel-artisan.makeException"
      },
      {
        "title": "Create factory",
        "command": "laravel-artisan.makeFactory"
      },
      {
        "title": "Create job",
        "command": "laravel-artisan.makeJob"
      },
      {
        "title": "Create listener",
        "command": "laravel-artisan.makeListener"
      },
      {
        "title": "Create mail",
        "command": "laravel-artisan.makeMail"
      },
      {
        "title": "Create middleware",
        "command": "laravel-artisan.makeMiddleware"
      },
      {
        "title": "Create migration",
        "command": "laravel-artisan.makeMigration"
      },
      {
        "title": "Create model",
        "command": "laravel-artisan.makeModel"
      },
      {
        "title": "Create notification",
        "command": "laravel-artisan.makeNotification"
      },
      {
        "title": "Create observer",
        "command": "laravel-artisan.makeObserver"
      },
      {
        "title": "Create policy",
        "command": "laravel-artisan.makePolicy"
      },
      {
        "title": "Create provider",
        "command": "laravel-artisan.makeProvider"
      },
      {
        "title": "Create request",
        "command": "laravel-artisan.makeRequest"
      },
      {
        "title": "Create resource",
        "command": "laravel-artisan.makeResource"
      },
      {
        "title": "Create rule",
        "command": "laravel-artisan.makeRule"
      },
      {
        "title": "Create scope",
        "command": "laravel-artisan.makeScope"
      },
      {
        "title": "Create seeder",
        "command": "laravel-artisan.makeSeeder"
      },
      {
        "title": "Create test",
        "command": "laravel-artisan.makeTest"
      },
      {
        "title": "Create view",
        "command": "laravel-artisan.makeView"
      },
      { "separator": true },
      {
        "title": "Open Controller",
        "command": "laravel-artisan.openControllers"
      },
      {
        "title": "Open Middleware",
        "command": "laravel-artisan.openMiddleware"
      },
      {
        "title": "Open Provider",
        "command": "laravel-artisan.openProviders"
      },
      {
        "title": "Open Model",
        "command": "laravel-artisan.openModels"
      },
      {
        "title": "Open Factory",
        "command": "laravel-artisan.openFactories"
      },
      {
        "title": "Open Migration",
        "command": "laravel-artisan.openMigrations"
      },
      {
        "title": "Open Seeder",
        "command": "laravel-artisan.openSeeders"
      },
      { "separator": true },
      {
        "title": "Open View",
        "command": "laravel-artisan.openInertiaPages"
      },
      {
        "title": "Browse Views",
        "command": "laravel-artisan.browseInertiaPages"
      },
      {
        "title": "Open Component",
        "command": "laravel-artisan.openInertiaComponents"
      },
      {
        "title": "Browse Components",
        "command": "laravel-artisan.browseInertiaComponents"
      },
      {
        "title": "Open Layout",
        "command": "laravel-artisan.openInertiaLayouts"
      },
      {
        "title": "Browse Layouts",
        "command": "laravel-artisan.browseInertiaLayouts"
      },
      {
        "title": "Open Livewire View",
        "command": "laravel-artisan.openLivewireViews"
      },
      {
        "title": "Browse Livewire Views",
        "command": "laravel-artisan.browseLivewireViews"
      },
      {
        "title": "Open Blade View",
        "command": "laravel-artisan.openBladeViews"
      },
      {
        "title": "Browse Blade Views",
        "command": "laravel-artisan.browseBladeViews"
      },
      {
        "title": "Open Blade Component",
        "command": "laravel-artisan.openBladeComponents"
      },
      {
        "title": "Browse Blade Components",
        "command": "laravel-artisan.browseBladeComponents"
      },
      {
        "title": "Open Livewire Component",
        "command": "laravel-artisan.openLivewireComponents"
      },
      {
        "title": "Browse Livewire Components",
        "command": "laravel-artisan.browseLivewireComponents"
      },
      { "separator": true },
      {
        "title": "Publish config",
        "command": "laravel-artisan.publishConfig"
      },
      {
        "title": "Publish all configs",
        "command": "laravel-artisan.publishAllConfigs"
      }
    ]
  },

  "config": [
    {
      "key": "laravel-artisan.silence.notifications",
      "title": "Mute notifications",
      "description": "Not recommended, but you do you.",
      "type": "boolean",
      "default": false
    },
    {
      "key": "laravel-artisan.shell.path",
      "title": "Shell",
      "description": "Full path to your preferred shell",
      "type": "string",
      "default": "/bin/sh"
    }
  ],
  "config-workspace": [
    {
      "key": "laravel-artisan.silence.notifications",
      "title": "Mute notifications",
      "description": "Not recommended, but you do you.",
      "type": "boolean",
      "default": false
    },
    {
      "key": "laravel-artisan.shell.path",
      "title": "Shell",
      "description": "Full path to your preferred shell",
      "type": "string",
      "default": "/bin/sh"
    }
  ]
}
