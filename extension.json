{
  "identifier": "teriyaki.LaravelArtisan",
  "name": "Laravel artisan",
  "organization": "Teriyaki",
  "description": "Lorem ipsum, dolor sit amet.",
  "version": "0.1",
  "categories": ["commands"],

  "main": "main.js",

  "activationEvents": ["onLanguage:php"],

  "entitlements": {
    "process": true,
    "filesystem": "readwrite"
  },

  "commands": {
    "extensions": [
      {
        "title": "Cache config",
        "command": "laravel-artisan.configCache"
      },
      {
        "title": "Clear config cache",
        "command": "laravel-artisan.configClear"
      },
      {
        "title": "Clear cache",
        "command": "laravel-artisan.cacheClear"
      },
      {
        "title": "Forget cache",
        "command": "laravel-artisan.cacheForget"
      },
      {
        "title": "Forget cahce",
        "command": "laravel-artisan.cacheForget"
      },
      { "separator": true },
      {
        "title": "Seed database",
        "command": "laravel-artisan.dbSeed"
      },
      {
        "title": "Wipe database",
        "command": "laravel-artisan.dbWipe"
      },
      {
        "title": "Migrate",
        "command": "laravel-artisan.migrate"
      },
      {
        "title": "Migrate fresh",
        "command": "laravel-artisan.migrateFresh"
      },
      {
        "title": "Migrate refresh",
        "command": "laravel-artisan.migrateRefresh"
      },
      {
        "title": "Migrate reset",
        "command": "laravel-artisan.migrateReset"
      },
      {
        "title": "Migrate rollback",
        "command": "laravel-artisan.migrateRollback"
      },
      { "separator": true },
      {
        "title": "Prune models",
        "command": "laravel-artisan.modelPrune"
      },
      {
        "title": "Create notifications migration",
        "command": "laravel-artisan.notificationsTable"
      },

      {
        "title": "Create cast",
        "command": "laravel-artisan.makeCast"
      },
      {
        "title": "Create channel",
        "command": "laravel-artisan.makeChannel"
      },
      {
        "title": "Create command",
        "command": "laravel-artisan.makeCommand"
      },
      {
        "title": "Create component",
        "command": "laravel-artisan.makeComponent"
      },
      {
        "title": "Create controller",
        "command": "laravel-artisan.makeController"
      },
      {
        "title": "Create event",
        "command": "laravel-artisan.makeEvent"
      },
      {
        "title": "Create exception",
        "command": "laravel-artisan.makeException"
      },
      {
        "title": "Create factory",
        "command": "laravel-artisan.makeFactory"
      },
      {
        "title": "Create job",
        "command": "laravel-artisan.makeJob"
      },
      {
        "title": "Create listener",
        "command": "laravel-artisan.makeListener"
      },
      {
        "title": "Create mail",
        "command": "laravel-artisan.makeMail"
      },
      {
        "title": "Create middleware",
        "command": "laravel-artisan.makeMiddleware"
      },
      {
        "title": "Create migration",
        "command": "laravel-artisan.makeMigration"
      },
      {
        "title": "Create model",
        "command": "laravel-artisan.makeModel"
      },
      {
        "title": "Create notification",
        "command": "laravel-artisan.makeNotification"
      },
      {
        "title": "Create observer",
        "command": "laravel-artisan.makeObserver"
      },
      {
        "title": "Create policy",
        "command": "laravel-artisan.makePolicy"
      },
      {
        "title": "Create provider",
        "command": "laravel-artisan.makeProvider"
      },
      {
        "title": "Create request",
        "command": "laravel-artisan.makeRequest"
      },
      {
        "title": "Create resource",
        "command": "laravel-artisan.makeResource"
      },
      {
        "title": "Create rule",
        "command": "laravel-artisan.makeRule"
      },
      {
        "title": "Create scope",
        "command": "laravel-artisan.makeScope"
      },
      {
        "title": "Create seeder",
        "command": "laravel-artisan.makeSeeder"
      },
      {
        "title": "Create test",
        "command": "laravel-artisan.makeTest"
      },
      {
        "title": "Create view",
        "command": "laravel-artisan.makeView"
      },
      { "separator": true }
    ]
  },

  "config": [
    {
      "key": "laravel-artisan.silence.notifications",
      "title": "Mute success messages",
      "description": "Not recommended, but you do you.",
      "type": "boolean",
      "default": false
    },
    {
      "key": "laravel-artisan.shell.path",
      "title": "Shell",
      "description": "Full path to your preferred shell",
      "type": "string",
      "default": "/bin/sh"
    }
  ]
}
